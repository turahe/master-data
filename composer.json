{
    "name": "turahe/master-data",
    "description": "Package Laravel yang berisi data Provinsi, Kabupaten/Kota, Kecamatan, dan Keluarahan/Desa di seluruh Master.",
    "keywords": [
        "laravel",
        "turahe",
        "address",
        "alamat",
        "indonesia",
        "provinsi",
        "kabupaten",
        "kota",
        "kecamatan",
        "kelurahan",
        "desa"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nur Wachid",
            "email": "wachid@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Turahe\\Master\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Turahe\\Master\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2.5",
        "illuminate/support": "5.* |^6.0 | ^7.0 | ^8.0",
      "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 |^8.5 | ^9.3",
        "php-coveralls/php-coveralls": "^2.1",
        "orchestra/testbench": "3.*|^4.0|^5.0",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "suggest": {
        "spatie/geocoder": "Synchronize latitude longitude data directly using Google's Geocoding Service"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
              "Turahe\\Master\\MasterServiceProvider"
            ],
            "aliases": {
                "Master": "Turahe\\Master\\MasterData"
            }
        }
    }
}
